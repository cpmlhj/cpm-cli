{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier":"error",
        "no-constant-condition": "off",
        "no-case-delcarations": "off"

    }
}
